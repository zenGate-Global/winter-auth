# Winter Authenticator Environment Variables
# Copy this file to .env and fill in your provider credentials

# Provider Configuration
WAUTH_PROVIDER=aws

# AWS Rekognition Credentials (when using AWS provider)
WAUTH_ACCESS_KEY=your_aws_access_key_here
WAUTH_SECRET_KEY=your_aws_secret_key_here
WAUTH_AWS_REGION=us-east-1

# Azure Face API Credentials (when using Azure provider)
# WAUTH_API_KEY=your_azure_api_key_here
# WAUTH_ENDPOINT=https://your-resource.cognitiveservices.azure.com/

# Kairos Credentials (when using Kairos provider) 
# WAUTH_API_KEY=your_kairos_api_key_here
# WAUTH_APP_ID=your_kairos_app_id_here
